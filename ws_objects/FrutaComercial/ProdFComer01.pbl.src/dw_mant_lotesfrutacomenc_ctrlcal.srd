 retrieve="  SELECT dba.spro_lotesfrutacomenc.lofc_pltcod,   
         dba.spro_lotesfrutacomenc.lofc_espcod,   
         dba.spro_lotesfrutacomenc.lofc_lotefc,   
         dba.spro_lotesfrutacomenc.prod_codigo,   
         dba.spro_lotesfrutacomenc.lofc_totbul,   
         dba.spro_lotesfrutacomenc.lofc_totkil,   
         dba.productores.prod_nombre,   
         dba.spro_lotesfrutacomenc.lofc_tipool  
    FROM dba.spro_lotesfrutacomenc,   
         dba.productores,   
         dba.spro_movtofrutacomdeta  
   WHERE ( dba.productores.prod_codigo = dba.spro_lotesfrutacomenc.prod_codigo ) and  
         ( dba.spro_movtofrutacomdeta.lofc_pltcod = dba.spro_lotesfrutacomenc.lofc_pltcod ) and  
         ( dba.spro_movtofrutacomdeta.lofc_espcod = dba.spro_lotesfrutacomenc.lofc_espcod ) and  
         ( dba.spro_movtofrutacomdeta.lofc_lotefc = dba.spro_lotesfrutacomenc.lofc_lotefc ) and  
         ( ( dba.spro_movtofrutacomdeta.plde_codigo = :Planta ) AND  
         ( dba.spro_movtofrutacomdeta.clie_codigo = :Cliente ) AND  
         ( dba.spro_movtofrutacomdeta.mfco_numero = :Numero ) AND  
         ( dba.spro_movtofrutacomdeta.mfcd_secuen = 1 ) AND  
         ( dba.spro_movtofrutacomdeta.tpmv_codigo = 12 ) )    
" arguments=(("Planta", number),("Cliente", number),("Numero", number)) )
