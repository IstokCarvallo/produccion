 column=(type=char(30) updatewhereclause=yes name=spro_calicosechero_cale_nombre dbname="spro_calicosechero.cale_nombre" dbalias=".cale_nombre" )
 retrieve="  SELECT dbo.envases.enva_nombre,   
         dbo.spro_movtoenvadeta.enva_tipoen,   
         dbo.spro_movtoenvadeta.enva_codigo,   
         dbo.spro_movtoenvadeta.fgme_cantid,
         dbo.spro_calicosechero.cale_nombre  
    FROM dbo.envases,   
         dbo.spro_movtoenvadeta,   
         dbo.spro_movtoenvaenca,
         dbo.spro_calicosechero  
   WHERE ( dbo.spro_movtoenvadeta.enva_tipoen = dbo.envases.enva_tipoen ) and  
         ( dbo.spro_movtoenvadeta.enva_codigo = dbo.envases.enva_codigo ) and  
			( dbo.spro_movtoenvadeta.fgme_sentid = 2 ) And
         ( dbo.spro_movtoenvaenca.plde_codigo = dbo.spro_movtoenvadeta.plde_codigo ) and  
         ( dbo.spro_movtoenvaenca.tpmv_codigo = dbo.spro_movtoenvadeta.tpmv_codigo ) and  
         ( dbo.spro_movtoenvaenca.meen_numero = dbo.spro_movtoenvadeta.meen_numero ) And
			( dbo.spro_movtoenvadeta.enva_tipoen = dbo.spro_calicosechero.enva_tipoen ) and  
         ( dbo.spro_movtoenvadeta.enva_codigo = dbo.spro_calicosechero.enva_codigo ) and
         ( dbo.spro_movtoenvadeta.cale_calida = dbo.spro_calicosechero.cale_calida ) and
         ( dbo.spro_movtoenvaenca.plde_codigo = :Planta ) And
  			( dbo.spro_movtoenvaenca.tpmv_codrec = :TipoMovto ) And
			( dbo.spro_movtoenvaenca.mfge_numero = :Numero )
" arguments=(("Planta", number),("TipoMovto", number),("Numero", number)) )
