 retrieve="  SELECT dbo.spro_ordenproceso.clie_codigo,   
         dbo.spro_ordenproceso.plde_codigo,   
         dbo.spro_ordenproceso.orpr_tipord,   
         dbo.spro_ordenproceso.orpr_numero,   
         dbo.spro_ordenproceso.prod_codigo,   
         dbo.productores.prod_nombre,   
         dbo.spro_ordenproceso.espe_codigo,   
         dbo.especies.espe_nombre,   
         dbo.spro_ordenproceso.vari_codigo,   
         dbo.variedades.vari_nombre ,   
         dbo.spro_ordenproceso.orpr_fecpro 
    FROM dbo.spro_ordenproceso,   
         dbo.especies,   
         dbo.productores,   
         dbo.variedades  
   WHERE ( dbo.spro_ordenproceso.espe_codigo = dbo.especies.espe_codigo ) and  
         ( dbo.spro_ordenproceso.prod_codigo = dbo.productores.prod_codigo ) and  
         ( dbo.spro_ordenproceso.espe_codigo = dbo.variedades.espe_codigo ) and  
         ( dbo.spro_ordenproceso.vari_codigo = dbo.variedades.vari_codigo ) and  
         :cliente in (dbo.spro_ordenproceso.clie_codigo, -1) and  
         :planta in (dbo.spro_ordenproceso.plde_codigo, -1) and  
         :tipo in (dbo.spro_ordenproceso.orpr_tipord, -1)
" arguments=(("cliente", number),("planta", number),("tipo", number))  sort="clie_codigo A plde_codigo A orpr_numero A " )
