 retrieve="  SELECT DISTINCT dba.spro_movtofrutacomdeta.lofc_pltcod,   
         dba.spro_movtofrutacomdeta.lofc_espcod,   
         dba.spro_movtofrutacomdeta.lofc_lotefc,   
			dba.spro_movtofrutacomdeta.lfcd_secuen,
         dba.spro_lotesfrutacomdeta.enva_tipoen,   
         dba.tiposenvases.tien_nombre,   
         dba.spro_lotesfrutacomdeta.enva_codigo,   
         dba.envases.enva_nombre,   
         dba.spro_movtofrutacomdeta.mfcd_bulent  
    FROM dba.envases,   
         dba.spro_movtofrutacomdeta,   
         dba.spro_movtofrutacomenca,   
         dba.tiposenvases,
		   dba.spro_lotesfrutacomdeta
   WHERE ( dba.spro_lotesfrutacomdeta.enva_tipoen = dba.tiposenvases.enva_tipoen ) and  
 			( dba.spro_lotesfrutacomdeta.enva_tipoen = dba.envases.enva_tipoen ) and  
         ( dba.spro_lotesfrutacomdeta.enva_codigo = dba.envases.enva_codigo ) and  
         ( dba.spro_movtofrutacomenca.plde_codigo = dba.spro_movtofrutacomdeta.plde_codigo ) and  
         ( dba.spro_movtofrutacomenca.tpmv_codigo = dba.spro_movtofrutacomdeta.tpmv_codigo ) and  
			( dba.spro_movtofrutacomenca.mfco_numero = dba.spro_movtofrutacomdeta.mfco_numero ) and
         ( dba.spro_movtofrutacomdeta.lofc_pltcod = dba.spro_lotesfrutacomdeta.lofc_pltcod ) and  
			( dba.spro_movtofrutacomdeta.lofc_espcod = dba.spro_lotesfrutacomdeta.lofc_espcod ) and  
         ( dba.spro_movtofrutacomdeta.lofc_lotefc = dba.spro_lotesfrutacomdeta.lofc_lotefc ) and  
         ( dba.spro_movtofrutacomdeta.lfcd_secuen = dba.spro_lotesfrutacomdeta.lfcd_secuen ) and  
         ( ( dba.spro_movtofrutacomenca.plde_codigo = :planta ) AND
			(  dba.spro_movtofrutacomenca.tpmv_codigo = 25 ) AND  
         ( dba.spro_movtofrutacomenca.mfco_tipdoc = :tipo ) AND  
         ( dba.spro_movtofrutacomenca.mfco_docrel = :orden ) )    
" arguments=(("planta", number),("tipo", number),("orden", number)) )
