 retrieve="SELECT  v1.zona_codigo ,
           v1.clie_codigo ,
           v1.espe_codigo ,
           v1.vari_codigo ,
           v1.vaes_secuen ,
           v1.vaca_calibr ,
           v1.prod_codigo ,
           v1.vaes_preuni ,
           v1.vaes_fecini ,
           v1.vaes_fecter ,
           dbo.productores.prod_nombre ,
           dbo.variedades.vari_nombre ,
		     v1.emba_codigo,
			  0 as todos,
			  0 as todoscal,
			  0 as semana
        FROM {oj dbo.valorespecial v1 LEFT OUTER JOIN dbo.productores   ON v1.zona_codigo = dbo.productores.zona_codigo AND v1.prod_codigo = dbo.productores.prod_codigo},
             {oj dbo.variedades   RIGHT OUTER JOIN dbo.valorespecial v2 ON dbo.variedades.espe_codigo = v2.espe_codigo  AND dbo.variedades.vari_codigo = v2.vari_codigo}     
        WHERE ( v1.clie_codigo = :Cliente ) 
and          ( v1.zona_codigo = :Zona )   " update="dbo.valorespecial" updatewhere=0 updatekeyinplace=yes arguments=(("Cliente", number),("Zona", number))  sort="zona_codigo A clie_codigo A espe_codigo A vari_codigo A prod_codigo A vaes_fecini A vaes_fecter A vaca_calibr A " )
