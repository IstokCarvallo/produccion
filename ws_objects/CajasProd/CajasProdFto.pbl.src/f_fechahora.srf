global function datetime f_fechahora ();Datetime ldt_FechaHora
Integer	li_Contador

SELECT	Count(*), GetDate()
	INTO	:li_Contador, :ldt_FechaHora
	FROM	dbo.admasistemas;

RETURN ldt_FechaHora
end function

