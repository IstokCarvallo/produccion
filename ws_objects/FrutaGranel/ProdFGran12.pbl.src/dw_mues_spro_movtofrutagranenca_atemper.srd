 retrieve="  SELECT dbo.spro_movtofrutagranenca.clie_codigo,   
         dbo.spro_movtofrutagranenca.plde_codigo,   
         dbo.spro_movtofrutagranenca.tpmv_codigo,   
         dbo.spro_movtofrutagranenca.mfge_numero,   
         dbo.spro_movtofrutagranenca.tran_codigo,   
         dbo.spro_movtofrutagranenca.espe_codigo,   
         dbo.spro_movtofrutagranenca.mfge_fecmov,   
         dbo.spro_movtofrutagranenca.mfge_guisii,   
         dbo.spro_movtofrutagranenca.mfge_estmov,
			dbo.spro_lotesfrutagranel.lote_codigo,
			dbo.spro_lotesfrutagranel.lote_guisii
    FROM dbo.spro_movtofrutagranenca, dbo.spro_movtofrutagrandeta, dbo.spro_lotesfrutagranel, dbo.spro_lotesfrutagranel_desverdizado
   WHERE dbo.spro_movtofrutagranenca.plde_codigo = dbo.spro_movtofrutagrandeta.plde_codigo AND  
         dbo.spro_movtofrutagranenca.tpmv_codigo = dbo.spro_movtofrutagrandeta.tpmv_codigo AND  
         dbo.spro_movtofrutagranenca.mfge_numero = dbo.spro_movtofrutagrandeta.mfge_numero AND 
 	      dbo.spro_movtofrutagranenca.clie_codigo = dbo.spro_movtofrutagrandeta.clie_codigo AND  
			dbo.spro_movtofrutagrandeta.lote_pltcod = dbo.spro_lotesfrutagranel.lote_pltcod AND
      	dbo.spro_movtofrutagrandeta.lote_espcod = dbo.spro_lotesfrutagranel.lote_espcod AND
      	dbo.spro_movtofrutagrandeta.lote_codigo = dbo.spro_lotesfrutagranel.lote_codigo AND
        dbo.spro_lotesfrutagranel.lote_pltcod   = dbo.spro_lotesfrutagranel_desverdizado.lote_pltcod AND
        dbo.spro_lotesfrutagranel.lote_espcod   = dbo.spro_lotesfrutagranel_desverdizado.lote_espcod AND
        dbo.spro_lotesfrutagranel.lote_codigo   = dbo.spro_lotesfrutagranel_desverdizado.lote_codigo AND
         ( dbo.spro_movtofrutagranenca.plde_codigo = :Planta ) AND  
         ( dbo.spro_movtofrutagranenca.tpmv_codigo = :TipoMovto ) AND 
 		   ( dbo.spro_movtofrutagranenca.clie_codigo = :Cliente ) AND 
         ( :Estado in (0, dbo.spro_movtofrutagranenca.mfge_estmov) ) AND  
         ( :Especie in (0, dbo.spro_movtofrutagranenca.espe_codigo) ) AND  
         ( dbo.spro_movtofrutagranenca.mfge_fecmov >= :FechaMovto ) AND
	    ( dbo.spro_lotesfrutagranel_desverdizado.frio_tipofr = :TipoFrio ) 
