 retrieve="  SELECT dbo.despafrigode.plde_codigo,   
         dbo.despafrigode.defe_numero,   
         dbo.despafrigode.clie_codigo,   
         dbo.despafrigode.paen_numero,   
         dbo.spro_palletencab.paen_tipopa,   
         dbo.variedades.vari_nombre,   
         dbo.spro_palletencab.cate_codigo,   
         dbo.spro_palletencab.paen_ccajas,   
         dbo.spro_palletencab.espe_codigo,   
         dbo.spro_palletencab.vari_codigo,   
         dbo.spro_palletencab.paen_inspec,   
         dbo.spro_palletencab.dest_codigo,   
         dbo.destinos.dest_nombre,   
         dbo.spro_palletencab.paen_concal,   
         dbo.despafrigode.defe_termog,   
         dbo.despafrigode.defe_tempe1,   
         dbo.despafrigode.defe_tempe2,   
         dbo.despafrigode.defe_ladoes,   
         dbo.despafrigode.defe_filaes,   
         dbo.despafrigode.defe_pcopda  
    FROM {oj dbo.spro_palletencab LEFT OUTER JOIN dbo.destinos ON dbo.spro_palletencab.dest_codigo = dbo.destinos.dest_codigo},   
         dbo.variedades,   
         dbo.despafrigode  
   WHERE ( dbo.variedades.espe_codigo = dbo.spro_palletencab.espe_codigo ) and  
         ( dbo.variedades.vari_codigo = dbo.spro_palletencab.vari_codigo ) and  
         ( dbo.despafrigode.clie_codigo = dbo.spro_palletencab.clie_codigo ) and  
         ( dbo.despafrigode.paen_numero = dbo.spro_palletencab.paen_numero ) and  
         ( dbo.despafrigode.plde_codigo = dbo.spro_palletencab.plde_codigo ) and  
         ( ( dbo.despafrigode.plde_codigo = :Planta ) AND  
         ( dbo.despafrigode.defe_numero = :Folio ) AND  
         ( dbo.spro_palletencab.clie_codigo = :cliente ) )    
" update="dbo.despafrigode" updatewhere=0 updatekeyinplace=yes arguments=(("cliente", number),("Planta", number),("Folio", number)) )
