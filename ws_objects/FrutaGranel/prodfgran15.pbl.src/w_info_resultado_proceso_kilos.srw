SetNull(li_Nula)

IF dwo.Name = 'espe_codigo' THEN
	IF iuo_Especie.Existe(Integer(data),True,SQLCA) THEN
		
		ii_especie = Integer(data)
		
		dw_variedad.GetChild('vari_codigo', idwc_variedad)
		idwc_variedad.SetTransObject(SQLCA)
		
		IF idwc_progproc.Retrieve(ii_planta, ii_especie, ii_cliente) = 0 THEN
			idwc_progproc.InsertRow(0)
		END IF
		
		IF idwc_variedad.Retrieve(ii_especie) = 0 THEN
			idwc_variedad.InsertRow(0)
		END IF
	ELSE
		This.SetItem(row,'espe_codigo',li_Nula)
		RETURN 1
	END IF
END IF
