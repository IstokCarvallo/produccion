public function boolean noexisteordenreproceso (long al_orden, integer ai_tipo);Integer	li_Especie, li_Planta, li_Estado, li_Linea, li_Nula, li_cliente,&
			li_Variedad, li_Seguimto, li_periodo
Date  	ldt_Fecha
Boolean	lb_Retorno = True
Long		ll_Productor, ll_ppre_numero

SetNull(li_Nula)

li_Planta	=	dw_Planta.Object.plde_codigo[1]
li_cliente 	= 	dw_2.Object.Clie_codigo[1]

SELECT	espe_codigo, dinp_estado, dinp_fecdoc, line_codigo
	INTO	:li_Especie, :li_Estado, :ldt_Fecha, :li_Linea
	FROM	dbo.spro_doctointernopack
	WHERE	plde_codigo	=	:li_Planta
	AND	dinp_tipdoc	=	:ai_tipo
	AND	dinp_numero	=	:al_Orden;
		 
IF sqlca.SQLCode = -1 THEN
	F_errorbasedatos(sqlca,"Lectura tabla spro_doctointernopack")
ELSEIF sqlca.SQLCode <> 100 THEN
//	iuo_Especie.Existe(li_Especie, False, sqlca,ii_cliente)
	iuo_Especie.Existe(li_Especie, False, sqlca)
	
	idwc_variedad.Retrieve(li_Especie)
	
	dw_especie.Object.espe_codigo[1]		=	li_Especie
	dw_linea.Object.line_codigo[1]		=	li_Linea
	dw_variedad.Object.vari_codigo[1]	=	li_Nula
	em_fecdes.text								=	String(Date(ldt_Fecha))
	em_fecter.text								=	String(Date(ldt_Fecha))
	sle_Seguimiento.Text						=	''

	dw_linea.Object.line_codigo.Protect							=	1
	dw_periodo.Object.pefr_codigo.Protect						=	1
	dw_especie.Object.espe_codigo.Protect						=	1
	dw_variedad.Object.vari_codigo.Protect						=	1
	dw_productor.Object.prod_codigo.Protect					=	1
	dw_spro_progproc.Object.ppre_numero.Protect			=	1
	dw_cont.Object.cont_codigo.Protect							=	1
	
	dw_linea.Object.line_codigo.Color					=	RGB(255,255,255)
	dw_periodo.Object.pefr_codigo.Color				=	RGB(255,255,255)
	dw_especie.Object.espe_codigo.Color			=	RGB(255,255,255)
	dw_variedad.Object.vari_codigo.Color			=	RGB(255,255,255)
	dw_productor.Object.prod_codigo.Color			=	RGB(255,255,255)
	dw_spro_progproc.Object.ppre_numero.Color	=	RGB(255,255,255)
	dw_cont.Object.cont_codigo.Color					=	RGB(255,255,255)
	
	dw_linea.Object.line_codigo.BackGround.Color					=	553648127
	dw_periodo.Object.pefr_codigo.BackGround.Color				=	553648127
	dw_especie.Object.espe_codigo.BackGround.Color			=	553648127
	dw_variedad.Object.vari_codigo.BackGround.Color			=	553648127
	dw_productor.Object.prod_codigo.BackGround.Color			=	553648127
	dw_spro_progproc.Object.ppre_numero.BackGround.Color	=	553648127
	dw_cont.Object.cont_codigo.BackGround.Color					=	553648127
		
	cbx_todasesp.Enabled	=	FALSE
	cbx_todaslin.Enabled		=	FALSE
	cbx_todosper.Enabled    	=  TRUE
	cbx_todasvar.Enabled		=	TRUE
	cbx_todosprod.Enabled	=	TRUE
	cbx_todosper.Checked    =  TRUE
	cbx_todasvar.Checked	=	TRUE
	cbx_todosprod.Checked	=	TRUE
	cbx_grupoprod.Enabled	=	TRUE
	cbx_consperiodo.Enabled = 	TRUE
	cbx_prodcons.Enabled	=	TRUE
	cbx_todasprog.Enabled	=	TRUE
	cbx_todcont.Enabled		=	TRUE
	cbx_todcont.Checked		=	TRUE
	em_fecdes.Enabled		=	FALSE
	lb_Retorno					=	FALSE
	
	ddlb_turnos.Enabled		=	False
	cbx_todtur.Enabled		=	TRUE
	cbx_todtur.Checked		=	TRUE
END IF

RETURN lb_Retorno
end function

public function boolean noexisteordenproceso (long al_orden);Integer	li_Especie, li_Planta, li_Estado, li_cliente, &
			li_Variedad, li_Seguimto, li_Linea, li_periodo, li_turno, li_cont
String	ls_Seguimiento[3]={'Productor','Huerto','Cuartel'} 
Date  	ldt_Fecha
Boolean	lb_Retorno = True
Long		ll_Productor

li_Planta	=	dw_Planta.Object.plde_codigo[1]
li_cliente 	= 	dw_2.Object.Clie_codigo[1]

SELECT espe_codigo, orpr_estado, orpr_fecpro, prod_codigo,
		vari_codigo, orpr_niveld, line_codigo, pefr_codigo, 
		ppre_numero, IsNull(orpr_nrotur, 0), cont_codigo
 INTO	:li_Especie, :li_Estado, :ldt_Fecha, :ll_Productor, 
		:li_Variedad, :li_Seguimto, :li_Linea, :li_periodo, 
		:il_ppre_numero, :li_turno, :li_cont
 FROM	dbo.spro_ordenproceso
WHERE	plde_codigo	=	:li_Planta
  AND	orpr_tipord	=	:ii_TipoOrden
  AND	orpr_numero	=	:al_Orden
  AND	clie_codigo	= 	:li_cliente;

IF sqlca.SQLCode = -1 THEN
	F_errorbasedatos(sqlca,"Lectura tabla Spro_Ordenproceso")
ELSEIF sqlca.SQLCode = 0 THEN
	iuo_Especie.Existe(li_Especie, False, sqlca)
	
	IF Not manbin_especie(li_Planta, li_Especie, True, sqlca) THEN
		Return lb_retorno
	END IF
	
	dw_variedad.GetChild("vari_codigo", idwc_variedad)
	idwc_variedad.SetTransObject(sqlca)
	idwc_variedad.Retrieve(li_Especie)
	
	dw_linea.Object.line_codigo[1]			=	li_Linea
	dw_periodo.Object.pefr_codigo[1]		=	li_Periodo
	dw_Especie.Object.espe_codigo[1]	=	li_Especie
	dw_variedad.Object.vari_codigo[1]	=	li_Variedad
	dw_productor.Object.prod_codigo[1]	=	ll_Productor
	dw_cont.Object.cont_codigo[1]			= 	li_cont
	
	ddlb_turnos.SelectItem(li_turno)
	
	dw_spro_progproc.GetChild("ppre_numero", idwc_progproc)
	idwc_progproc.SetTransObject(SQLCA)
	idwc_progproc.Retrieve(li_Planta, li_Especie, li_Cliente)
	dw_spro_progproc.Object.ppre_numero[1] = 	il_ppre_numero
	
	dw_linea.Object.line_codigo.Protect							=	1
	dw_periodo.Object.pefr_codigo.Protect						=	1
	dw_especie.Object.espe_codigo.Protect						=	1
	dw_variedad.Object.vari_codigo.Protect						=	1
	dw_productor.Object.prod_codigo.Protect					=	1
	dw_spro_progproc.Object.ppre_numero.Protect			=	1
	dw_cont.Object.cont_codigo.Protect							=	1
	
	dw_linea.Object.line_codigo.Color					=	RGB(255,255,255)
	dw_periodo.Object.pefr_codigo.Color				=	RGB(255,255,255)
	dw_especie.Object.espe_codigo.Color			=	RGB(255,255,255)
	dw_variedad.Object.vari_codigo.Color			=	RGB(255,255,255)
	dw_productor.Object.prod_codigo.Color			=	RGB(255,255,255)
	dw_spro_progproc.Object.ppre_numero.Color	=	RGB(255,255,255)
	dw_cont.Object.cont_codigo.Color					=	RGB(255,255,255)
	
	dw_linea.Object.line_codigo.BackGround.Color					=	553648127
	dw_periodo.Object.pefr_codigo.BackGround.Color				=	553648127
	dw_especie.Object.espe_codigo.BackGround.Color			=	553648127
	dw_variedad.Object.vari_codigo.BackGround.Color			=	553648127
	dw_productor.Object.prod_codigo.BackGround.Color			=	553648127
	dw_spro_progproc.Object.ppre_numero.BackGround.Color	=	553648127
	dw_cont.Object.cont_codigo.BackGround.Color					=	553648127
	
	cbx_todasesp.Enabled	=	False
	cbx_todasvar.Enabled		=	False
	cbx_todaslin.Enabled		=	False
	cbx_todosper.Enabled		=	False
	cbx_todosprod.Enabled	=	False
	cbx_grupoprod.Enabled	=	False
	cbx_prodcons.Enabled	=	False
	em_fecdes.Enabled		=	False
	cbx_todasprog.Enabled	=	False
	
	cbx_todcont.Checked		=	False
	cbx_todcont.Enabled		=	False
	
	ddlb_turnos.Enabled		=	False
	cbx_todtur.Enabled		=	False
	cbx_todtur.Checked		=	False

	IF Isnull(li_Seguimto) OR li_Seguimto < 1 THEN li_Seguimto = 1
	
	em_fecdes.text			=	String(ldt_Fecha,'dd/mm/yyyy')
	em_fecter.text			=	String(ldt_Fecha,'dd/mm/yyyy')
	sle_Seguimiento.Text	=	ls_Seguimiento[li_Seguimto]
	
	IF li_Seguimto = 1 THEN
		cbx_predio.Checked	=	True
	END IF
	
	lb_Retorno	=	False
END IF

RETURN lb_Retorno
end function

public function boolean existecontratista (integer ai_valor);Integer	li_existe, li_codigo
Boolean 	lb_retorno

lb_retorno	=	TRUE
li_codigo		=	ai_valor

SELECT Count(*) 
INTO :li_existe FROM dbo.contratista
WHERE cont_codigo = :li_codigo;

IF sqlca.SQLCode = -1 THEN
	F_ErrorBaseDatos(sqlca, "Lectura de Tabla de Contratistas")
	lb_Retorno	=	False
ELSEIF SQLCA.SQLCode = 100 THEN
	lb_Retorno	=	False
ELSE
	IF isnull(li_existe) or li_existe=0 THEN lb_Retorno	=	False 	
END IF

Return lb_retorno
end function

