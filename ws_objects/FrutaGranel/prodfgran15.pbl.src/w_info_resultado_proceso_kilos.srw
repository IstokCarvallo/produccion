event pb_acepta::clicked;SetPointer(HourGlass!)

Long		ll_Fila, 	ll_Orden,   ll_Productor, li_contratista, li_turno
Integer  	li_Planta,  li_Especie, li_Variedad,  li_ConsPredio,  li_TipoInforme, &
			li_Linea,   li_Periodo, li_ConsFecha
Date		ld_fecdes,  ld_fecter
Dec{2}   	ld_peso

li_Planta		=	dw_Planta.Object.plde_codigo[1]
ld_Fecdes	=	Date(left(em_fecdes.Text, 10))
ld_Fecter		=	Date(left(em_fecter.Text, 10))
ii_cliente		=	dw_2.Object.clie_codigo[1]

If IsNull(li_planta) Then
	MessageBox("Error de Datos", "Falta el Ingreso de Planta.")
	Return
End If

If IsNull(ii_TipoOrden) Then
	MessageBox("Error de Datos", "Falta el Ingreso del Tipo de Proceso.")
	Return
End If

If ii_TipoOrden = 0 or cbx_todosproc.Checked Then
	ll_Orden = -1
Else
	ll_Orden		=	Long(em_Proceso.text)
	If IsNull(ll_Orden) Then
		MessageBox("Error de Datos", "Falta el Ingreso de un Número de Orden.")
		Return
	End If
End If

If rb_InfoProd.Checked Then
	li_TipoInforme			=	2
	istr_info.Titulo		=	"Informe Proceso en Kilos al Productor"
Else
	li_TipoInforme			=	1
	istr_info.Titulo		=	"Informe Interno de Proceso en Kilos"
End If

If cbx_todosprod.Checked Then
	ll_Productor			=	-1
	If cbx_prodcons.Checked Then
		ll_Productor		=	-9
	ElseIf cbx_grupoprod.Checked Then
		ll_Productor		= 	-8
	End If
Else
	ll_Productor			=	dw_Productor.Object.prod_codigo[1]

	If Isnull(ll_Productor) Then
		MessageBox("Atención","Falta seleccionar el Productor")
		Return
	End If
End If

If cbx_todasesp.Checked Then
	li_Especie				=	-1
Else
	li_Especie				=	dw_especie.Object.espe_codigo[1]

	If Isnull(li_Especie) Then
		MessageBox("Atención","Falta seleccionar la Especie")
		Return
	End If
End If

If cbx_todcont.Checked Then
	If cbx_conscont.Checked AND cbx_conscont.Visible Then
		li_contratista		=	-9
	Else
		li_contratista		=	-1
	End If
Else
	If dw_cont.Object.cont_codigo.Protect = '1' Then
		li_contratista		=	-1
	Else
		li_contratista		=	dw_cont.Object.cont_codigo[1]

		If Isnull(li_contratista) Then
			MessageBox("Atención","Falta seleccionar Contratista")
			Return
		End If
	End If
End If

If cbx_todtur.Checked Then
	If cbx_constur.Checked AND cbx_constur.Visible Then
		li_turno		=	-9
	Else
		li_turno		=	-1
	End If
Else
	li_turno	=	Integer(Right(ddlb_turnos.Text, 1))

	If Isnull(li_turno) OR li_turno = 0 Then
		MessageBox("Atención","Falta seleccionar Turno")
		Return
	End If
End If

If cbx_todasvar.Checked Then
	li_Variedad		=	-1
	If cbx_consvari.Checked Then li_Variedad	=	-9
Else
	li_Variedad		=	dw_variedad.Object.vari_codigo[1]

	If Isnull(li_Variedad) Then
		MessageBox("Atención","Falta seleccionar la Variedad")
		Return
	End If
End If

If cbx_todaslin.Checked Then
	li_Linea			=	-1
Else
	li_Linea			=	dw_linea.Object.line_codigo[1]
	If Isnull(li_Linea) Then
		MessageBox("Atención","Falta seleccionar la Línea de Packing")
		Return
	End If
End If

If cbx_todosper.Checked Then
	li_Periodo	=	-1
	If cbx_consperiodo.Checked Then
		li_Periodo = -9
	End If
Else
	li_Periodo	=	dw_periodo.Object.pefr_codigo[1]
	If Isnull(li_Periodo) Then
		MessageBox("Atención","Falta seleccionar el Periodo")
		Return
	End If
End If

If cbx_predio.Checked Then li_ConsPredio	= 1
If cbx_consfecha.Checked Then li_ConsFecha	= 1

If cbx_Resumen.Checked Then
	ld_peso = Round(DEC(em_peso.text), 2)
	
	If isnull(ld_peso) OR ld_peso=0 Then
		MessageBox("Atención","Falta ingresar un peso estándar.")
		Return
	End If

	If ld_peso<0 Then
		MessageBox("Atención","El peso estándar debe ser un número mayor a cero.")
		Return
	End If
	em_peso.text = String(ld_peso)
Else
	ld_peso = 1
End If	

If Not cbx_todasprog.Checked and isnull(il_ppre_numero) Then
	MessageBox("Atención","Debe seleccionar un Programa de Proceso.")
	Return
ElseIf cbx_todasprog.Checked Then
	il_ppre_numero = -1
End If

istr_info.Copias	=	1

OpenWithParm(vinf, istr_info)

If cbx_com.Checked Then
	dw_infcom.SetTransObject(SQLCA)
	dw_infcom.Retrieve(li_planta,ii_TipoOrden,ll_orden,ii_cliente)
	dw_infcom.Print()
End If

If cbx_1.Checked Then
	If cbx_2.Checked Then
		Vinf.dw_1.DataObject =	"dw_info_procesokilos_pordias_lotes"
	ElseIf cbx_3.Checked Then
		Vinf.dw_1.DataObject =	"dw_info_procesokilos_pordias_comercial"
	Else
		Vinf.dw_1.DataObject =	"dw_info_procesokilos_pordias"
	End If

	Vinf.dw_1.SetTransObject(sqlca)
	ll_Fila	=	vinf.dw_1.Retrieve(ii_cliente,     li_planta,     ii_TipoOrden, ll_orden,       ll_Productor,&
											 li_Especie,     li_Variedad,   li_Linea,     li_Periodo,     ld_fecdes,&
											 ld_fecter,      li_ConsPredio, li_ConsFecha, li_TipoInforme, il_ppre_numero,&
											 li_contratista, li_turno)
Else
	If ii_TipoOrden = 8 Then
		Vinf.dw_1.DataObject =	"dw_info_preprocesokilos_final"
	ElseIf ii_TipoOrden = 5 Then
		Vinf.dw_1.DataObject =	"dw_info_reprocesokilos_final"
	Else
		Vinf.dw_1.DataObject =	"dw_info_procesokilos_final"
	End If
	
	Vinf.dw_1.SetTransObject(sqlca)
	ll_Fila	=	vinf.dw_1.Retrieve(ii_cliente,    li_planta,      ll_orden,      ii_TipoOrden, ll_Productor,&
											 li_Especie,    li_Variedad,    li_Linea,      li_Periodo,   ld_fecdes, &
											 ld_fecter,     il_ppre_numero, li_ConsPredio, li_ConsFecha, &
											 li_TipoInforme,li_contratista, li_turno)

End If

If ll_Fila = -1 Then
	MessageBox( "Error en Base de Datos", "Se ha producido un error en Base de datos : ~n" + sqlca.SQLErrText, StopSign!, Ok!)
ElseIf ll_Fila = 0 Then
	MessageBox( "No Existe información", "No existe información para este informe.", StopSign!, Ok!)
Else
	F_Membrete(vinf.dw_1)
	ParamTemporada(gstr_paramtempo)
	vinf.dw_1.ModIfy("nombre_temporada.text = '" + gstr_paramtempo.nombre + "'")
	If gs_Ambiente <> 'Windows' Then  F_ImprimeInformePdf(vinf.dw_1, istr_info.titulo)
End If

SetPointer(Arrow!)
end event

