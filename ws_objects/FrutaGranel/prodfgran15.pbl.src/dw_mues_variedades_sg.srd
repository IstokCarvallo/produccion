 retrieve="SELECT  dbo.variedades.espe_codigo ,
		dbo.variedades.vari_codigo ,
		dbo.variedades.vari_nombre ,
		dbo.variedades.grva_codigo ,
		dbo.variedades.grva_codsub ,
		dbo.especies.espe_nombre ,
		dbo.variedades.enva_tipoen ,
		dbo.variedades.enva_codigo ,
		dbo.envases.enva_nombre ,
		grupovariedad_a.grva_nombre ,
		grupovariedad_b.grva_nombre ,
		dbo.tiposenvases.tien_nombre
FROM  dbo.variedades LEFT OUTER JOIN dbo.envases ON dbo.variedades.enva_tipoen = dbo.envases.enva_tipoen
												    AND dbo.variedades.enva_codigo = dbo.envases.enva_codigo	
					LEFT OUTER JOIN dbo.grupovariedad grupovariedad_b ON dbo.variedades.espe_codigo = grupovariedad_b.espe_codigo
																	AND dbo.variedades.grva_codigo = grupovariedad_b.grva_codigo,
      dbo.especies, dbo.tiposenvases, dbo.grupovariedad grupovariedad_a
WHERE dbo.especies.espe_codigo = dbo.variedades.espe_codigo
and   dbo.variedades.espe_codigo = grupovariedad_a.espe_codigo
and   dbo.variedades.grva_codigo = grupovariedad_a.grva_codigo
and   dbo.variedades.grva_codsub = grupovariedad_a.grva_codsub
And   dbo.tiposenvases.enva_tipoen = dbo.envases.enva_tipoen
and   :espe_codigo in (0,dbo.variedades.espe_codigo )
and   grupovariedad_b.grva_codsub = 0
order by dbo.variedades.vari_codigo " arguments=(("espe_codigo", number))  sort="vari_codigo A " )
