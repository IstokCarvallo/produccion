table(column=(type=decimal(0) update=yes updatewhereclause=yes key=yes name=plde_codigo dbname="spro_correladhesivos.plde_codigo" )
 column=(type=decimal(0) update=yes updatewhereclause=yes key=yes name=coad_embala dbname="spro_correladhesivos.coad_embala" )
 column=(type=decimal(0) updatewhereclause=yes name=crta_cantid dbname="crta_cantid" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=coad_secuen dbname="spro_correladhesivos.coad_secuen" )
 column=(type=char(77) updatewhereclause=yes name=pers_nombre dbname="pers_nombre" )
 column=(type=decimal(0) updatewhereclause=yes name=tipo dbname="tipo" initial="0" values="Ninguno	0/Seleccionadora	1/Pesadora	2/Embaladora	3/Cosecha	4/" )
 column=(type=char(1) updatewhereclause=yes name=lado dbname="lado" values="Lado A	A/Lado B	B/Lado C	C/Sin Lado	./" )
 column=(type=decimal(0) updatewhereclause=yes name=cantidad dbname="cantidad" initial="4" values="X4	4/X5	5/" )
 retrieve="  SELECT dbo.spro_correladhesivos.plde_codigo,   
         dbo.spro_correladhesivos.coad_embala,   
         crta_cantid=(convert(decimal(6,0),0)),   
         dbo.spro_correladhesivos.coad_secuen,   
         (RTrim(pers_nombre) + ' ' + RTrim(pers_apepat) + ' ' + RTrim(pers_apemat)) as pers_nombre,   
         tipo=convert(numeric(1), 0),   
         lado=convert(char(1), '.'),   
         cantidad=convert(numeric(1), 4)  
    FROM dbo.spro_correladhesivos,   
         dbo.remupersonal  
   WHERE ( dbo.spro_correladhesivos.coad_embala = dbo.remupersonal.pers_nrotar ) and  
         ( ( dbo.spro_correladhesivos.plde_codigo = :Planta ) AND  
         ( dbo.spro_correladhesivos.coad_embala = :Embaladora ) )    
" update="dbo.spro_correladhesivos" updatewhere=0 updatekeyinplace=yes arguments=(("Planta", number),("Embaladora", number)) )
