 column=(type=decimal(0) updatewhereclause=yes name=defe_guides dbname="despafrigoen.defe_guides" dbalias="de.defe_guides" )
 column=(type=date updatewhereclause=yes name=defe_fecdes dbname="despafrigoen.defe_fecdes" dbalias="de.defe_fecdes" )
