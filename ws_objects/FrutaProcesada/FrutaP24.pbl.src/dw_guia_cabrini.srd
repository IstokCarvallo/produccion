 column=(type=char(40) updatewhereclause=yes name=puer_nombre dbname="puer_nombre" )
 retrieve="SELECT de.clie_codigo, de.plde_codigo, de.defe_numero, de.defe_guides, de.defe_fecdes, 
		de.defe_plasag, puer_nombre = Rtrim(pu.puer_nombre)
    FROM dbo.guiasnuevas gn Inner Join dbo.despafrigoen de
            On gn.plde_codigo = de.plde_codigo
           And gn.defe_numero = de.defe_numero
           And gn.nueva = de.defe_guides,
           dbo.embarqueprod em, dbo.puertos pu
where de.embq_codigo = em.embq_codigo
  And de.clie_codigo = em.clie_codigo
  And em.puer_codigo = pu.puer_codigo" )
