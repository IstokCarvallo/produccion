 retrieve="  SELECT dbo.controldespachos.clie_codigo,   
         dbo.controldespachos.plde_codigo,   
         dbo.controldespachos.defe_numero,   
         dbo.controldespachos.usua_codigo,   
         dbo.controldespachos.code_fechaa,   
         dbo.controldespachos.code_horaap,   
         dbo.controldespachos.code_motivo,   
         dbo.controldespachos.code_gemsaa,   
         dbo.controldespachos.code_gengde,   
         dbo.controldespachos.code_gemide,   
         dbo.controldespachos.code_demade,   
         dbo.despafrigoen.defe_estado  
    FROM dbo.controldespachos,   
         dbo.despafrigoen  
   WHERE ( dbo.controldespachos.clie_codigo = dbo.despafrigoen.clie_codigo ) and  
         ( dbo.controldespachos.plde_codigo = dbo.despafrigoen.plde_codigo ) and  
         ( dbo.controldespachos.defe_numero = dbo.despafrigoen.defe_numero ) and  
         ( ( dbo.controldespachos.clie_codigo = :cliente ) AND  
         ( dbo.controldespachos.plde_codigo = :planta ) AND  
         ( IsNull(dbo.despafrigoen.defe_estado,1) = 1 or dbo.despafrigoen.defe_estado = 2 or dbo.despafrigoen.defe_estado = 0) )    
" update="dbo.controldespachos" updatewhere=0 updatekeyinplace=yes arguments=(("cliente", number),("planta", number))  sort="defe_numero D " )
