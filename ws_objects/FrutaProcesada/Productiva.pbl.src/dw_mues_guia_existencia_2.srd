 retrieve="  SELECT  ~"dba~".~"exismovtoenca~".~"mden_gdprod~" ,           
(~"dba~".~"exismovtoenca~".~"mden_numero~") as mden_numero,           
~"dba~".~"exismovtoenca~".~"bode_codigo~" ,           
~"dba~".~"exismovtoenca~".~"mden_fecmov~" ,           
~"dba~".~"exismovtoenca~".~"tpmv_tipomv~" ,           
sum(~"dba~".~"exismovtodeta~".~"mdde_cantid~") as mdde_cantid ,           
~"dba~".~"exismovtodeta~".~"item_codigo~" ,           
~"dba~".~"exismovtoenca~".~"mden_bodest~" ,           
~"dba~".~"exismovtoenca~".~"mden_docrel~" ,
~"dba~".~"itembodega~".~"enva_tipoen~",
~"dba~".~"itembodega~".~"enva_codigo~",
~"dba~".~"exismovtoenca~".~"tpmv_codigo~"     
FROM ~"dba~".~"exismovtoenca~" ,  ~"dba~".~"exismovtodeta~",
~"dba~".~"itembodega~"      
WHERE ( ~"dba~".~"exismovtoenca~".~"mden_tipdoc~" = ~"dba~".~"exismovtodeta~".~"mden_tipdoc~" ) and          
( ~"dba~".~"exismovtoenca~".~"mden_numero~" = ~"dba~".~"exismovtodeta~".~"mden_numero~" ) and          
( ~"dba~".~"exismovtoenca~".~"mden_gdprod~" = :guia ) and          
( ~"dba~".~"exismovtoenca~".~"mden_estado~" = 1 ) and          
( ~"dba~".~"exismovtoenca~".~"mden_docrel~" = :docrel ) and          
( ~"dba~".~"exismovtoenca~".~"mden_tipdoc~" = 1 ) and          
( ~"dba~".~"exismovtoenca~".~"tpmv_tipomv~" = 1 ) And          
( ~"dba~".~"exismovtoenca~".~"tpmv_codigo~" in (2,16) ) and  
(~"dba~".~"itembodega~".~"item_codigo~" = ~"dba~".~"exismovtodeta~".~"item_codigo~") and
(~"dba~".~"exismovtoenca~".~"mden_fecmov~" = :fecha) and
:item in ('-1',~"dba~".~"exismovtodeta~".~"item_codigo~")
GROUP BY ~"dba~".~"exismovtoenca~".~"mden_gdprod~" ,           
~"dba~".~"exismovtoenca~".~"mden_numero~" ,           
~"dba~".~"exismovtoenca~".~"bode_codigo~" ,           
~"dba~".~"exismovtoenca~".~"mden_fecmov~" ,           
