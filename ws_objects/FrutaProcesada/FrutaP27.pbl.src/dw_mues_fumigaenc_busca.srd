 retrieve="  SELECT dbo.fumigaenc.clie_codigo,   
         dbo.fumigaenc.plde_codigo,   
         dbo.fumigaenc.fumi_numero,   
         dbo.fumigaenc.fumi_fecfum,   
         dbo.fumigaenc.fumi_cantar,   
         dbo.fumigaenc.fumi_cantcaj,   
         dbo.fumigaenc.fumi_horain,   
         dbo.fumigaenc.cama_codigo,   
         dbo.fumigaenc.fumi_observ,   
         dbo.fumigaenc.cond_codigo,   
         dbo.fumigaenc.fumi_numsag,   
         isnull(dbo.fumigaenc.fumi_estado,0) as fumi_estado  
    FROM dbo.fumigaenc  
   WHERE ( :cliente in (-1,dbo.fumigaenc.clie_codigo) ) AND  
         ( dbo.fumigaenc.plde_codigo = :planta ) AND  
         :condicion in (-1,dbo.fumigaenc.cond_codigo)    
" update="dbo.fumigaenc" updatewhere=0 updatekeyinplace=yes arguments=(("cliente", number),("planta", number),("condicion", number))  sort="fumi_numero D fumi_fecfum A plde_codigo A clie_codigo A " )
