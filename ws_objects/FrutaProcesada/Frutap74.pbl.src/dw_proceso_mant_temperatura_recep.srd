 retrieve="  SELECT DISTINCT dbo.recfruproced.paen_numero,   
         dbo.recfruproced.rfpe_tempe1,   
         dbo.recfruproced.rfpe_tempe2,   
         dbo.variedades.vari_nombre,   
         dbo.especies.espe_nombre,   
         dbo.recfruproced.plde_codigo,   
         dbo.recfruproced.rfpe_numero,   
         dbo.recfruproced.clie_codigo,   
         dbo.palletencab.espe_codigo,   
         dbo.palletencab.vari_codigo  
    FROM dbo.recfruproced,   
         dbo.variedades,   
         dbo.especies,   
         dbo.palletencab  
   WHERE ( dbo.variedades.espe_codigo = dbo.especies.espe_codigo ) and  
         ( dbo.recfruproced.clie_codigo = dbo.palletencab.clie_codigo ) and  
         ( dbo.recfruproced.paen_numero = dbo.palletencab.paen_numero ) and  
         ( dbo.recfruproced.plde_codigo = dbo.palletencab.plde_codigo ) and  
         ( dbo.palletencab.espe_codigo = dbo.especies.espe_codigo ) and  
         ( dbo.palletencab.vari_codigo = dbo.variedades.vari_codigo ) and  
         ( ( dbo.recfruproced.clie_codigo = :cliente ) AND  
         ( dbo.recfruproced.plde_codigo = :planta ) AND  
         ( dbo.recfruproced.rfpe_numero = :numero ) )    
" update="dbo.recfruproced" updatewhere=0 updatekeyinplace=yes arguments=(("cliente", number),("planta", number),("numero", number)) )
