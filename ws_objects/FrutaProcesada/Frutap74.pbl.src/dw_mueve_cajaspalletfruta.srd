 retrieve="  SELECT pal1.clie_codigo,   
         pal1.paen_numero,   
         pal1.espe_codigo,   
         pal1.vari_codigo,   
         pal1.emba_codigo,   
         pal1.prod_codigo,   
         pal1.cond_codigo,   
         pal1.etiq_codigo,   
         pal1.plde_codigo,   
         pal1.pafr_calibr,   
         pal1.pafr_secuen,   
         pal1.pafr_ccajas,   
         pal1.pafr_nrlote,   
         pal1.pafr_copack,   
         pal1.pafr_varrot,   
			pal1.pafr_prdrot,   
         pal1.pafr_calrot,   
         pal1.pafr_fecemb,   
         pal1.pafr_fecing,   
         pal1.pafr_fecdes,   
         pal1.pafr_cjssal,   
         pal1.pafr_huert1,   
         pal1.pafr_huert2,   
         pal1.pafr_huert3,   
         pal1.pafr_huert4,   
         pal1.pafr_cuart1,   
         pal1.pafr_cuart2,   
         pal1.pafr_cuart3,   
         pal1.pafr_cuart4,   
         pal1.pafr_barra1,   
         pal1.pafr_barra2,   
         pal1.pafr_barra3,   
         pal1.pafr_barra4,   
         pal1.tran_fechat,   
         pal1.pafr_rotpak,   
         pal1.pafr_docrel,   
         pal1.pafr_tipdoc,   
         pal1.pafr_fecrot,   
         pal1.pafr_nroori,   
         pal1.pafr_secori,   
         pal1.pafr_embrea  
    FROM dbo.palletfruta as pal1,   
         dbo.palletfruta as pal2 
   WHERE pal1.clie_codigo = pal2.clie_codigo  and  
         pal1.plde_codigo = pal2.plde_codigo  and  
         pal1.pafr_secuen = pal2.pafr_secuen  and  
         pal1.clie_codigo = :cliente  AND  
         pal1.plde_codigo = :planta  AND  
         pal1.paen_numero = :numero  AND  
         pal2.paen_numero = :numero2 AND
			pal1.pafr_ccajas = 0  and
			pal1.pafr_secuen > 0  
             
" update="dbo.palletfruta" updatewhere=0 updatekeyinplace=yes arguments=(("cliente", number),("planta", number),("numero", number),("numero2", number)) )
