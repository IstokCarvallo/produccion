 retrieve="  SELECT dbo.repalletenca.repe_numero,   
         dbo.repalletenca.repe_fecrep,   
         dbo.repalletenca.repe_observ,   
         dbo.repalletenca.repe_factur,   
         dbo.repalletenca.plde_codigo,   
         dbo.repalletenca.clie_codigo,   
         dbo.repalletenca.repe_nrosag  
    FROM dbo.repalletenca  
   WHERE ( dbo.repalletenca.clie_codigo = :cliente ) AND  
         ( dbo.repalletenca.plde_codigo = :planta ) AND  
         ( dbo.repalletenca.repe_fecrep between :fecdesde and :fechasta ) AND  
         ( :filtro in ('-1',dbo.repalletenca.repe_observ) )    
" update="dbo.repalletenca" updatewhere=0 updatekeyinplace=yes arguments=(("cliente", number),("planta", number),("fecdesde", date),("fechasta", date),("filtro", string))  sort="repe_numero D " )
