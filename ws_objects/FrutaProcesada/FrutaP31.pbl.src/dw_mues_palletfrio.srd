 column=(type=decimal(0) update=yes updatewhereclause=yes name=teen_numero dbname="TERMOMETRIADETA.teen_numero" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=paen_numero dbname="TERMOMETRIADETA.paen_numero" )
 column=(type=char(40) updatewhereclause=yes name=cama_nombre dbname="camarasbode.cama_nombre" )
 retrieve="  SELECT DISTINCT dbo.termometriadeta.plde_codigo,   
         dbo.termometriadeta.cama_codigo,   
         dbo.termometriadeta.teen_numero,   
         dbo.termometriadeta.paen_numero,   
         dbo.camarasbode.cama_nombre  
    FROM dbo.termometriadeta,   
         dbo.camarasbode,   
         dbo.termometriaenca  
   WHERE ( dbo.termometriadeta.plde_codigo = dbo.camarasbode.plde_codigo ) and  
         ( dbo.termometriadeta.cama_codigo = dbo.camarasbode.cama_codigo ) and  
         ( dbo.termometriaenca.plde_codigo = dbo.termometriadeta.plde_codigo ) and  
         ( dbo.termometriaenca.cama_codigo = dbo.termometriadeta.cama_codigo ) and  
         ( dbo.termometriaenca.teen_numero = dbo.termometriadeta.teen_numero ) and  
         ( ( dbo.termometriadeta.plde_codigo = :Planta ) AND  
         ( :Camara in (-1, dbo.termometriaenca.cama_codigo) ) AND  
         ( :Proceso in (-1, dbo.termometriaenca.teen_numero) ) AND  
         ( :Pallet in (-1, dbo.termometriadeta.paen_numero) ) AND  
         ( DateDiff(dd, dbo.termometriaenca.teen_fechas , :Fecha) = 0 ) )    
" arguments=(("Planta", number),("Camara", number),("Proceso", number),("Pallet", number),("Fecha", date)) )
