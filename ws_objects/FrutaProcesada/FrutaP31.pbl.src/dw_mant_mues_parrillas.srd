 column=(type=decimal(0) updatewhereclause=yes name=rfpe_nrores dbname="recfruprocee.rfpe_nrores" )
 column=(type=date updatewhereclause=yes name=rfpe_fecrec dbname="recfruprocee.rfpe_fecrec" )
 column=(type=decimal(2) update=yes updatewhereclause=yes name=paen_tempal dbname="palletencab.PAEN_TEMPAL" )
 retrieve="  SELECT dbo.palletencab.clie_codigo,   
         dbo.palletencab.paen_numero,   
         dbo.palletencab.plde_codigo,   
         dbo.palletencab.paen_tipopa,   
         dbo.palletencab.paen_conpal,   
         dbo.palletencab.paen_conpar,   
         dbo.recfruproced.rfpe_numero,   
         dbo.recfruprocee.rfpe_nrores,   
         dbo.recfruprocee.rfpe_fecrec,   
         dbo.palletencab.PAEN_TEMPAL  
    FROM dbo.palletencab, dbo.recfruproced, dbo.recfruprocee  
   WHERE ( dbo.palletencab.clie_codigo = dbo.recfruproced.clie_codigo ) and  
         ( dbo.palletencab.plde_codigo = dbo.recfruproced.plde_codigo ) and  
         ( dbo.palletencab.paen_numero = dbo.recfruproced.paen_numero ) and  
         ( dbo.recfruproced.plde_codigo = dbo.recfruprocee.plde_codigo ) and  
         ( dbo.recfruproced.rfpe_numero = dbo.recfruprocee.rfpe_numero ) and  
         ( dbo.recfruproced.clie_codigo = dbo.recfruprocee.clie_codigo ) and  
         ( ( :Planta in (-1, dbo.recfruprocee.plde_codigo) ) AND  
         ( :Cliente in (-1, dbo.recfruprocee.clie_codigo) ) AND  
         ( :Numero in (-1, dbo.recfruprocee.rfpe_numero) ) AND 
		( :Fecha in(Convert(Date, '19000101'), dbo.recfruprocee.rfpe_fecrec) )And
         ( :Guia in (-1, dbo.recfruprocee.rfpe_nrores) ) )    
" update="dbo.palletencab" updatewhere=0 updatekeyinplace=yes arguments=(("Planta", number),("Cliente", number),("Fecha", date),("Numero", number),("Guia", number))  sort="rfpe_numero A rfpe_nrores A rfpe_fecrec A paen_numero A " )
