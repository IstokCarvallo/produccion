 retrieve="  SELECT dbo.plantillatrazabilidad.esta_codigo,   
         dbo.plantillatrazabilidad.acti_codigo,   
         dbo.plantillatrazabilidad.plde_codigo,   
         dbo.plantillatrazabilidad.ubic_codigo,   
         dbo.plantillatrazabilidad.espe_codigo,   
         dbo.plantillatrazabilidad.vari_codigo,   
         dbo.plantillatrazabilidad.clie_codigo,   
         dbo.plantillatrazabilidad.plac_tiemax,   
         dbo.plantillatrazabilidad.plac_tiemin,   
         dbo.plantillatrazabilidad.plac_activo,   
         dbo.plantillatrazabilidad.plac_fechas,   
         dbo.plantillatrazabilidad.usua_codigo  
    FROM dbo.plantillatrazabilidad  
   WHERE ( :Estacion in (-1, dbo.plantillatrazabilidad.esta_codigo) ) AND  
         ( :Planta in (-1, dbo.plantillatrazabilidad.plde_codigo) ) AND  
         ( :Cliente in (-1, dbo.plantillatrazabilidad.clie_codigo) ) AND  
         ( :Especie in (-1, dbo.plantillatrazabilidad.espe_codigo) )    
" update="dbo.plantillatrazabilidad" updatewhere=1 updatekeyinplace=yes arguments=(("Cliente", number),("Planta", number),("Estacion", number),("Especie", number)) )
