 retrieve="  SELECT dbo.repalletenca.plde_codigo,   
         dbo.repalletenca.repe_numero,   
         dbo.repalletenca.clie_codigo,   
         dbo.repalletenca.repe_fecrep,   
         dbo.repalletenca.inpr_numero,   
         dbo.repalletenca.repe_cantar,   
         dbo.repalletenca.repe_cancaj,   
         dbo.repalletenca.repe_cajmer,   
         dbo.repalletenca.repe_tipopa,   
         dbo.repalletenca.repe_lugarp,   
         dbo.repalletenca.repe_nrosag  
    FROM dbo.repalletenca  
   WHERE dbo.repalletenca.clie_codigo = :cliente
      AND  dbo.repalletenca.plde_codigo = :planta
	 And IsNUll(dbo.repalletenca.repe_nrosag, 0) <> 0
" update="dbo.repalletenca" updatewhere=0 updatekeyinplace=yes arguments=(("cliente", number),("planta", number))  sort="repe_numero D " )
