 retrieve="  SELECT dbo.repalletenca.plde_codigo,   
         dbo.repalletenca.repe_numero,   
         dbo.repalletenca.clie_codigo,   
         dbo.repalletenca.repe_fecrep,   
         dbo.repalletenca.inpr_numero,   
         dbo.repalletenca.repe_cantar,   
         dbo.repalletenca.repe_tardef,   
         dbo.repalletenca.repe_cancaj,   
         dbo.repalletenca.repe_cajmer,   
         dbo.repalletenca.repe_tipopa,   
         dbo.repalletenca.repe_lugarp,   
         dbo.repalletenca.repe_fecact,   
         dbo.repalletenca.repe_horact,   
         dbo.repalletenca.repe_respon,   
         dbo.repalletenca.repe_observ,   
         dbo.repalletenca.repe_usuari,   
         dbo.repalletenca.repe_comput,   
         dbo.repalletenca.repe_camalt,   
         dbo.repalletenca.repe_nrosag  
    FROM dbo.repalletenca  
   WHERE ( dbo.repalletenca.plde_codigo = :planta ) AND  
         ( dbo.repalletenca.clie_codigo = :cliente )   
			order by  repe_numero desc
" update="dbo.repalletenca" updatewhere=0 updatekeyinplace=no arguments=(("planta", number),("cliente", number)) )
