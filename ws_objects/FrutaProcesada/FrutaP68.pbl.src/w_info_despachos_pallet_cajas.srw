event open;call super::open;Boolean lb_Cerrar

If IsNull(uo_SelCliente.Codigo) Then lb_Cerrar = True
If IsNull(uo_SelEspecie.Codigo) Then lb_Cerrar = True
If IsNull(uo_SelPlanta.Codigo) Then lb_Cerrar = True
If IsNull(uo_SelPuerto.Codigo) Then lb_Cerrar = True
If IsNull(uo_SelVariedad.Codigo) Then lb_Cerrar = True
If IsNull(uo_SelMercado.Codigo) Then lb_Cerrar = True
If IsNull(uo_SelDestino.Codigo) Then lb_Cerrar = True
If IsNull(uo_SelRecibidor.Codigo) Then lb_Cerrar = True

iuo_calibre   						=	Create uo_calibre

If lb_Cerrar Then
	Close(This)
Else
	uo_SelCliente.Seleccion(True, False)
	uo_SelEspecie.Seleccion(True, True)
	uo_SelPlanta.Seleccion(True, True)
	uo_SelPuerto.Seleccion(True, True)
	uo_SelVariedad.Seleccion(True, True)
	uo_SelMercado.Seleccion(True, True)
	uo_SelDestino.Seleccion(True, True)
	uo_SelRecibidor.Seleccion(True, True)
	
	uo_SelCliente.Todos(False)
	uo_SelCliente.Codigo = gi_CodExport
	uo_SelCliente.dw_Seleccion.Object.Codigo[1] = gi_CodExport
		
	dw_etiqueta.GetChild("etiq_codigo", idwc_etiqueta)
	idwc_etiqueta.SetTransObject(sqlca)
	idwc_etiqueta.Retrieve()
	dw_etiqueta.InsertRow(0)
	dw_etiqueta.Enabled										=	False
	dw_etiqueta.Object.etiq_codigo.BackGround.Color	=	RGB(166,180,210)
	
	dw_operaciones.GetChild("oper_codigo", idwc_operaciones)
	idwc_operaciones.SetTransObject(SQLCA)
	idwc_operaciones.Retrieve(gi_CodExport)
	dw_operaciones.InsertRow(0)
	dw_operaciones.Enabled											=	False
	dw_operaciones.Object.oper_codigo.BackGround.Color	=	RGB(166,180,210)
	
	em_desde.Text					=	'01/01/1900'
	em_hasta.Text					=	String(Today())
	istr_mant.argumento[6]		= 	'**'
	istr_mant.argumento[8]		= 	'**'
	istr_mant.argumento[7]	= 	'-9'
	istr_mant.argumento[33]	= 	'-9'
End If
end event

