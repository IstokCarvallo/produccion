event pb_acepta::clicked;SetPointer(HourGlass!)

Integer	fila, li_cliente, li_Rotula = 0, li_etiqueta, li_operacion, li_incluye = 0, li_emba, li_emba2, li_consolida = 0
String		ls_embalaje, ls_construyelike,ls_string, ls_string2, ls_construyelike1, ls_construye

istr_info.titulo	= 'INFORME DE DESPACHOS PALLET / CAJAS'
OpenWithParm(vinf, istr_info)
vinf.dw_1.DataObject = "dw_info_despacho_pallet_cajas"
vinf.dw_1.SetTransObject(sqlca)

ls_embalaje	=	istr_mant.argumento[6]+','
li_emba = len(ls_embalaje)

For li_emba2 = 1 To li_emba
	ls_string = mid(ls_embalaje,li_emba2,1)
	If ls_string <> ',' Then
		ls_construye = ls_construye+ls_string
	Else 
		If ls_construyelike1 = '' Then
			ls_construyelike1 = ' emba_codigo like '+''+"'%"+ls_construye+"%'"+''
			ls_construye = ''
		Else	
			If ls_construyelike = '' Then
				ls_construyelike = ls_construyelike1 +' or emba_codigo like '+''+"'%"+ls_construye+"%'"+''
			Else
				ls_construyelike = ls_construyelike +' or emba_codigo like '+''+"'%"+ls_construye+"%'"+''
			End If
			ls_construye = ''
		End If	
	End If	
Next

If cbx_operacion.Checked=False Then istr_mant.argumento[33]	=	String(dw_operaciones.Object.oper_codigo[1])
If ls_construyelike = '' Then ls_construyelike = ls_construyelike1
If cbx_rotula.Checked Then li_Rotula = 1
If cbx_excluye.Checked Then li_incluye = 1	
If cbx_fecemb.Checked Then li_consolida = 1
If cbx_embalaje.Checked Or cbx_consembalaje.Checked Then ls_construyelike = "'Z' = 'Z'"

fila	=	vinf.dw_1.Retrieve(uo_SelCliente.Codigo, uo_SelEspecie.Codigo, uo_SelPlanta.Codigo, uo_SelVariedad.Codigo, uo_SelPuerto.Codigo,&
				Date(em_Desde.Text), Date(em_Hasta.Text), Long(istr_mant.argumento[33]), istr_mant.argumento[6], Integer(istr_mant.argumento[7]), &
				istr_mant.argumento[8], uo_SelMercado.Codigo, uo_SelDestino.Codigo, uo_SelRecibidor.Codigo, li_incluye, li_Rotula, ls_construyelike, li_consolida)

If fila = -1 Then
	MessageBox("Error en Base de Datos", "Se ha producido un error en Base de datos : ~n" + sqlca.SQLErrText, StopSign!, Ok!)
ElseIf fila = 0 Then
	MessageBox( "No Existe información", "No existe información para este informe.", StopSign!, Ok!)
Else
	F_Membrete(vinf.dw_1)
	IF gs_Ambiente <> 'Windows' THEN
		F_ImprimeInformePdf(vinf.dw_1, istr_info.titulo)
	END IF
End If

SetPointer(Arrow!)				
end event

