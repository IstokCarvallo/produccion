 retrieve="  SELECT dbo.despafrigoen.clie_codigo,   
         dbo.despafrigoen.plde_codigo,   
         dbo.despafrigoen.defe_numero,   
         dbo.despafrigoen.defe_fecdes,   
         dbo.despafrigoen.defe_guides,   
         dbo.despafrigoen.embq_codigo  
    FROM dbo.despafrigoen   
  WHERE :Planta in (-1, dbo.despafrigoen.plde_codigo) 
	AND  :Cliente in (-1, dbo.despafrigoen.clie_codigo)
	AND   :Numero in (-1, dbo.despafrigoen.defe_numero)
	AND  :Fecha in(Convert(Date, '19000101'), dbo.despafrigoen.defe_fecdes)
	And	:Guia in (-1, dbo.despafrigoen.defe_guides)
" update="dbo.despafrigoen" updatewhere=0 updatekeyinplace=yes arguments=(("Planta", number),("Cliente", number),("Numero", number),("Fecha", date),("Guia", number)) )
