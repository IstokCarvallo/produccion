end type

event pb_acepta::clicked;SetPointer(Arrow!)

Integer	fila, li_tipopallemba= -1, li_cliente, li_condicion, li_varirotula, li_prdrot, li_calrot, &
			li_secuencia, li_emba, li_emba2, li_contemba, li_orden
String		l_s_titulo, ls_tipopal = 'PALLETS COMPLETOS', ls_embalaje,ls_condicion, ls_revision, &
			ls_sort = "clie_codigo A, plde_codigo A, paen_numero A, paen_fecemb A",&
			ls_orden, ls_ccalidad, ls_fechas,lS_tipopallemba, ls_lista, ls_string, ls_construye,&
			ls_construyelike, ls_construyelike1, ls_embasolo

li_cliente	=	Integer(istr_mant.argumento[2])
ls_embalaje	=	istr_mant.argumento[8]+','
ls_embasolo =  istr_mant.argumento[8]

If rb_pucho.Checked Then ls_tipopal	=	'PUCHOS'
If rb_ambas.Checked Then ls_tipopal	=	'PALLET Y PUCHOS'

li_emba = len(ls_embalaje)

FOR li_emba2 = 1 TO li_emba
	ls_string = mid(ls_embalaje,li_emba2,1)
	If ls_string <> ',' Then
		ls_construye = ls_construye+ls_string
	Else
		If ls_construyelike1 = '' Then
			ls_construyelike1 = ' emba_codigo like '+''+"'%"+ls_construye+"%'"+''
			ls_construye = ''
		Else	
			If ls_construyelike = '' Then
				ls_construyelike = ls_construyelike1 +' or emba_codigo like '+''+"'%"+ls_construye+"%'"+''
			Else
				ls_construyelike = ls_construyelike +' or emba_codigo like '+''+"'%"+ls_construye+"%'"+''
			End If
			ls_construye = ''
		End If	
	End If	
NEXT	

If ls_construyelike = '' Then ls_construyelike = ls_construyelike1

//Condición
If uo_selcondicion.cbx_todos.checked Then
	li_condicion	= -1
	ls_condicion   = 'Todas'
Else
	li_condicion	= uo_SelCondicion.Codigo
   	If IsNull(li_condicion)Then
		MessageBox("Atención","Debe Seleccionar una Condición Previamente",Exclamation!)
	   	Return
	Else
		ls_condicion   = String(li_condicion)+' '+uo_SelCondicion.Nombre
	End If
End If
/*
Categoria
*/
If IsNull(uo_SelCate.Codigo)Then
	MessageBox("Atención","Debe Seleccionar una Categoria Previamente",Exclamation!)
	uo_SelCate.dw_Seleccion.SetFocus()
	Return
End If
/*
productor
*/
ls_lista = uo_selproductor.Lista
/*
Especies
*/
If IsNull(uo_selespecie.Codigo)Then
	MessageBox("Atención","Debe Seleccionar una Especie Previamente",Exclamation!)
	uo_selespecie.dw_Seleccion.SetFocus()
	Return
End If
/*
Variedad
*/
If IsNull(uo_selvariedad.Codigo)Then
	MessageBox("Atención","Debe Seleccionar una Variedad Previamente",Exclamation!)
	uo_selvariedad.dw_Seleccion.SetFocus()
	Return
End If

If ll_norden = 0 OR isnull(ll_norden) Then
	MessageBox("Atención","Debe Seleccionar Nº Orden de Proceso Previamente",Exclamation!)
	em_norden.SetFocus()
	Return
End If	

If istr_mant.argumento[11]='0' Then
	ls_revision	=	''
ElseIf istr_mant.argumento[11]='1' Then
	ls_revision	   =	'INSPECCIONADOS'
//	ls_condicion	=	'NO FUMIGADOS'
ElseIf istr_mant.argumento[11]='2' Then
	ls_revision	   =	'NO Inspeccionados'
//   ls_condicion	=	'NO FUMIGADOS'
ElseIf istr_mant.argumento[11]='4' Then
	ls_revision	   =	'ReInspeccionados'
//   ls_condicion	=	'NO FUMIGADOS'
ElseIf istr_mant.argumento[11]='5' Then
	ls_revision	   =	'PENDIENTES'
//   ls_condicion	=	'NO FUMIGADOS'	
Else
	ls_revision	   =	'RECHAZADOS'
//   ls_condicion	=	'NO FUMIGADOS'	
End If

If rb_controltodos.Checked Then
	ls_ccalidad = ''
ElseIf rb_habilitado.Checked Then
	ls_ccalidad = 'HABILITADOS'
ElseIf rb_rechazados.Checked Then
	ls_ccalidad = 'RECHAZADOS'
Else
	ls_ccalidad = 'OBJETADOS'
End If
 
If cbx_consolid.Checked Then
	istr_mant.argumento[21]	= '0'
	istr_mant.argumento[22] = "CONSOLIDADOS"
ElseIf cbx_status.Checked Then
	istr_mant.argumento[21]	= '-1'
	istr_mant.argumento[22] = "TODOS"
End If	

If cbx_varirotula.Checked Then
	li_varirotula = 1
Else
	li_varirotula = 0
End If	

//Código de Pallet
If cbx_pallet.Checked Then
	istr_mant.argumento[24]	= string(-1)
End If

If cbx_secuencia.Checked Then
	il_secuencia = -1
Else	
	il_secuencia = dw_descripcion.Object.dsag_secuen[1]
End If	

ls_fechas = 'Embalados Entre el : '+istr_mant.argumento[9]+' y el '+istr_mant.argumento[10]

l_s_titulo				= 'Existencia'
istr_info.titulo		='Fruta Procesada ' + l_s_titulo 

OpenWithParm(vinf, istr_info)

If cbx_destinos.Checked Then
	vinf.dw_1.DataObject = "dw_info_existencia_palletpucho_destinospro"
	ls_orden	=	'PALLET'
Else	
	If rb_fecha.Checked Then
		//ls_sort = "clie_codigo A, paen_fecemb A, paen_numero A"
		If istr_mant.argumento[11]	=	'1' Then
			vinf.dw_1.DataObject = "dw_info_existencia_palletpucho_fechas_in"
			ls_orden	=	'FECHA'
		Else	 
			vinf.dw_1.DataObject = "dw_info_existencia_palletpucho_fechas"
			ls_orden	=	'FECHA'
		End If
		li_orden = 2
	ElseIf rb_pallet.Checked Then
		If istr_mant.argumento[11]	=	'1' Then
			vinf.dw_1.DataObject = "dw_info_existencia_palletpucho_inspec"	
			ls_orden	=	'PALLET'
		Else	
			vinf.dw_1.DataObject = "dw_info_existencia_palletpucho"	
			ls_orden	=	'PALLET'
		End If
		li_orden = 1
	ElseIf rb_embalaje.Checked Then
		If istr_mant.argumento[11]	=	'1' Then
			vinf.dw_1.DataObject = "dw_info_existencia_palletpucho_embj_in"	
			ls_orden	=	'EMBALAJE'
		Else	
			vinf.dw_1.DataObject = "dw_info_existencia_palletpucho_embj"	
			ls_orden	=	'EMBALAJE'
		End If
		li_orden = 4
	ElseIf rb_variedad.Checked Then
		If istr_mant.argumento[11]	=	'1' Then
			vinf.dw_1.DataObject = "dw_info_existencia_palletpucho_vari_in"	
			ls_orden	=	'VARIEDAD'
		Else	
			vinf.dw_1.DataObject = "dw_info_existencia_palletpucho_vari"	
			ls_orden	=	'VARIEDAD'
		End If
		li_orden = 5
	Else
		If istr_mant.argumento[11]	=	'1' Then
			vinf.dw_1.DataObject = "dw_info_existencia_palletpucho_calibr_in"	
			ls_orden	=	'CALIBRE'
		Else	
			vinf.dw_1.DataObject = "dw_info_existencia_palletpucho_calibr"	
			ls_orden	=	'CALIBRE'
		End If	
		li_orden = 3
	End If
End If
vinf.dw_1.SetTransObject(sqlca)

If cbx_embalaje.Checked Then
	ls_construyelike = "'Z' = 'Z'"
End If

If cbx_tipoembalaje.Checked Then
	li_tipopallemba = -1
Else
	li_tipopallemba = Integer(em_tipoemba.Text)	
End If

If cbx_prdrot.Checked Then
	li_prdrot 	=	1
Else
	li_prdrot	=	0
End If

If cbx_calrot.Checked Then
	li_calrot 	=	1
Else
	li_calrot	=	0
End If

//istr_mant.argumento[8] = 'Z'

fila	=	vinf.dw_1.Retrieve(Integer(istr_mant.argumento[1]),Integer(istr_mant.argumento[2]), &
			Integer(istr_mant.argumento[12]), uo_selespecie.Codigo,uo_selvariedad.Codigo,&
			Integer(istr_mant.argumento[6]),istr_mant.argumento[7],&
			istr_mant.argumento[8], Date(istr_mant.argumento[9]),Date(istr_mant.argumento[10]),&
			Integer(istr_mant.argumento[11]),ii_variable,li_condicion, Integer(li_tipopallemba),&
			Integer(istr_mant.argumento[20]),Integer(istr_mant.argumento[21]),&
			Integer(istr_mant.argumento[24]),uo_SelCate.Codigo,li_varirotula,ll_norden,li_prdrot,li_calrot,&
			is_frio,ii_tipoi,ls_lista,ii_calIficacion,il_secuencia,ls_construyelike,li_orden,String(ii_calIficacion2),&
			Integer(istr_mant.argumento[31]),Integer(istr_mant.argumento[32]),Integer(istr_mant.argumento[33]),&
			Integer(istr_mant.argumento[34]),Integer(istr_mant.argumento[35]))

If fila = -1 Then
	MessageBox( "Error en Base de Datos", "Se ha producido un error en Base " + &
			   	"de datos : ~n" + sqlca.SQLErrText, StopSign!, Ok!)
ElseIf fila = 0 Then
	MessageBox( "No Existe información", "No existe información para este informe.", StopSign!, Ok!)
Else
	F_Membrete(vinf.dw_1)
	vinf.dw_1.Object.tipo_pallet.Text	=	ls_tipopal
	vinf.dw_1.ModIfy("condicion.text = '" + ls_condicion+ "'")
	vinf.dw_1.ModIfy("revision.text = '" + ls_revision+ "'")
	vinf.dw_1.ModIfy("orden.text = '" + ls_orden+ "'")
	vinf.dw_1.ModIfy("ccalidad.text = '" + ls_ccalidad+ "'")
	vinf.dw_1.ModIfy("fechas.text = '" + ls_fechas+ "'")	
	istr_mant.argumento[8] = em_embalaje.Text
	If gs_Ambiente <> 'Windows' Then F_ImprimeInformePdf(vinf.dw_1, istr_info.titulo)
End If

SetPointer(Arrow!)
end event

