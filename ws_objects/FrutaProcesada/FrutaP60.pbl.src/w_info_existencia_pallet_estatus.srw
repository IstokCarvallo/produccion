integer y = 1208
integer height = 172
integer taborder = 420
boolean bringtotop = true
end type

on uo_seleszona.destroy
call uo_seleccion_zonas::destroy
end on

event ue_cambio;call super::ue_cambio;IF IsNull(This.Codigo) THEN RETURN

CHOOSE CASE This.Codigo
	CASE -1, -9
		uo_selproductor.cbx_Todos.Checked		=	True
		uo_selproductor.cbx_Consolida.Enabled	=	True
		uo_selproductor.Enabled						=	False
	
	CASE ELSE
		uo_selproductor.Enabled		=	True
		uo_selproductor.Filtra(uo_seleszona.Codigo,-1,Integer(istr_mant.argumento[2]))

END CHOOSE

		
end event

