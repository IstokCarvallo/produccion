 retrieve="  SELECT dbo.palletencab.paen_numero,   
         dbo.palletencab.emba_codigo,   
         dbo.palletencab.espe_codigo,   
         dbo.palletencab.vari_codigo,   
         dbo.variedades.vari_nombre,   
         dbo.palletencab.dest_codigo,   
         dbo.palletencab.paen_ccajas,   
         dbo.palletencab.paen_inspec,   
         dbo.palletencab.clie_codigo,   
         dbo.palletencab.plde_codigo,   
         dbo.inspecpaldet.inpd_fechai,   
         dbo.inspecpaldet.inpe_tipoin,   
         dbo.inspecpaldet.inpe_numero,   
         dbo.inspecpaldet.inpd_frecha,   
         dbo.palletencab.inpe_tipoin,   
         dbo.palletencab.inpe_numero,   
         dbo.palletencab.inpe_fechai,   
         dbo.palletencab.paen_fecter  
    FROM dbo.palletencab,   
         dbo.variedades,   
         dbo.inspecpaldet  
   WHERE ( dbo.variedades.espe_codigo = dbo.palletencab.espe_codigo ) and  
         ( dbo.variedades.vari_codigo = dbo.palletencab.vari_codigo ) and  
         ( dbo.inspecpaldet.clie_codigo = dbo.palletencab.clie_codigo ) and  
         ( dbo.inspecpaldet.plde_codigo = dbo.palletencab.plde_codigo ) and  
         ( dbo.inspecpaldet.paen_numero = dbo.palletencab.paen_numero ) and  
         ( ( :Cliente in (-1,dbo.palletencab.clie_codigo)) AND  
         ( dbo.palletencab.plde_codigo = :Planta ) AND  
         ( dbo.inspecpaldet.inpe_numero = :Folio ) AND  
         ( dbo.palletencab.paen_inspec in (3) ) )    
" update="dbo.palletencab" updatewhere=0 updatekeyinplace=yes arguments=(("Cliente", number),("Planta", number),("Folio", number)) )
