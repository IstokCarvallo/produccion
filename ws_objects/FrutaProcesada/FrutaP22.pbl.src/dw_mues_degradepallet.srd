 retrieve="  SELECT DISTINCT dbo.palletfruta.vari_codigo,   
         dbo.palletfruta.pafr_varrot,   
         variedades_a.vari_nombre,   
         dbo.palletfruta.paen_numero,   
         dbo.palletfruta.clie_codigo,   
         dbo.palletfruta.plde_codigo,   
         dbo.palletfruta.prod_codigo,   
         dbo.palletfruta.espe_codigo,   
         dbo.palletfruta.emba_codigo,   
         dbo.palletfruta.cond_codigo,   
         dbo.palletfruta.etiq_codigo,   
         dbo.palletfruta.pafr_secuen,   
         dbo.palletfruta.pafr_calibr,   
         dbo.palletfruta.pafr_ccajas,   
         dbo.palletfruta.pafr_nrlote,   
         dbo.palletfruta.pafr_copack,   
         variedades_b.vari_nombre,   
         dbo.palletfruta.pafr_prdrot,   
         dbo.palletfruta.pafr_calrot,   
         dbo.palletfruta.pafr_huert4,   
         dbo.palletfruta.pafr_cuart4,   
         dbo.palletfruta.pafr_rotpak,   
         dbo.palletfruta.pafr_huert1,   
         dbo.palletfruta.pafr_cuart1,
			dbo.palletfruta.pafr_fecemb,
			dbo.palletfruta.pafr_fecrot,
			dbo.palletfruta.pafr_embrea,
         dbo.palletfruta.pafr_cuart3
    FROM dbo.palletencab,   
         dbo.palletfruta,   
         dbo.variedades variedades_a,   
         dbo.variecalibre,   
         dbo.variedades variedades_b  
   WHERE ( dbo.palletencab.clie_codigo = dbo.palletfruta.clie_codigo ) and  
         ( dbo.palletencab.paen_numero = dbo.palletfruta.paen_numero ) and  
         ( dbo.palletencab.plde_codigo = dbo.palletfruta.plde_codigo ) and  
         ( dbo.palletfruta.espe_codigo = variedades_a.espe_codigo ) and  
         ( dbo.palletfruta.vari_codigo = variedades_a.vari_codigo ) and  
         ( variedades_a.espe_codigo = dbo.variecalibre.espe_codigo ) and  
         ( variedades_a.vari_codigo = dbo.variecalibre.vari_codigo ) and  
         ( dbo.variecalibre.espe_codigo = variedades_b.espe_codigo ) and  
         ( dbo.palletfruta.pafr_varrot = variedades_b.vari_codigo ) and  
         ( ( dbo.palletfruta.paen_numero = :numero ) AND  
         ( dbo.palletencab.clie_codigo = :cliente ) AND  
         ( dbo.palletencab.plde_codigo = :planta ) )  and
			dbo.palletfruta.pafr_ccajas > 0  
" update="dbo.palletfruta" updatewhere=0 updatekeyinplace=yes arguments=(("numero", number),("cliente", number),("planta", number))  sort="paen_numero A " )
