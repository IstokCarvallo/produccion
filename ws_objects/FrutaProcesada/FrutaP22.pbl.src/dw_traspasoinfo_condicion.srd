table(column=(type=decimal(0) updatewhereclause=yes name=clie_codigo dbname="palletencab.clie_codigo" )
 column=(type=decimal(0) updatewhereclause=yes name=paen_numero dbname="palletencab.paen_numero" )
 column=(type=char(60) updatewhereclause=yes name=vari_nombre dbname="variedades.vari_nombre" )
 column=(type=decimal(0) updatewhereclause=yes name=paen_inspec dbname="palletencab.paen_inspec" )
 column=(type=decimal(0) updatewhereclause=yes name=paen_ccajas dbname="palletencab.paen_ccajas" )
 column=(type=char(10) updatewhereclause=yes name=emba_codigo dbname="palletfruta.emba_codigo" )
 retrieve="  SELECT DISTINCT dbo.palletencab.clie_codigo,   
         dbo.palletencab.paen_numero,   
         dbo.variedades.vari_nombre,   
         dbo.palletencab.paen_inspec,   
         dbo.palletencab.paen_ccajas,   
         dbo.palletfruta.emba_codigo  
    FROM dbo.palletencab,   
         dbo.palletfruta,   
         dbo.variedades  
   WHERE ( dbo.palletencab.clie_codigo = dbo.palletfruta.clie_codigo ) and  
         ( dbo.palletencab.paen_numero = dbo.palletfruta.paen_numero ) and  
         ( dbo.palletencab.plde_codigo = dbo.palletfruta.plde_codigo ) and  
         ( dbo.palletfruta.espe_codigo = dbo.variedades.espe_codigo ) and  
         ( dbo.palletfruta.vari_codigo = dbo.variedades.vari_codigo ) and  
         ( ( dbo.palletencab.clie_codigo = :cliente ) AND  
         ( dbo.palletencab.plde_codigo = :planta ) AND  
         ( :categoria in (-1,dbo.palletfruta.cate_codigo) ) AND  
         ( :especie in (-1,dbo.palletfruta.espe_codigo) ) AND  
         ( :variedad in (-1,dbo.palletfruta.vari_codigo) ) AND  
         ( :calibre in ('-1',dbo.palletfruta.pafr_calibr) ) AND  
         ( :embalaje in ('-1', dbo.palletfruta.emba_codigo) ) ) AND  
         dbo.palletencab.paen_estado not in (2,3) AND  
         dbo.palletfruta.pafr_ccajas > 0   AND
			dbo.palletfruta.cond_codigo = 0  
" arguments=(("especie", number),("variedad", number),("categoria", number),("calibre", string),("embalaje", string),("cliente", number),("planta", number)) )
