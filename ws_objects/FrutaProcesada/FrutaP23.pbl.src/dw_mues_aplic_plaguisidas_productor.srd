 retrieve="  SELECT dbo.especies.espe_nombre,   
         dbo.productores.prod_nombre,   
         dbo.sagdeclaproddeta.clie_codigo,   
         dbo.sagdeclaproddeta.plde_codigo,   
         dbo.sagdeclaproddeta.espe_codigo,   
         dbo.sagdeclaproddeta.prod_codigo,   
         dbo.sagdeclaproddeta.decl_fecdec,   
         dbo.sagdeclaproddeta.decl_predio,   
         dbo.sagdeclaproddeta.decl_secuen,   
         dbo.sagdeclaproddeta.decl_fecapl,   
         dbo.sagdeclaproddeta.decl_procom,   
         dbo.sagdeclaproddeta.decl_ingact,   
         dbo.sagdeclaproddeta.decl_apldos  
    FROM dbo.especies,   
         dbo.productores,   
         dbo.sagdeclaproddeta  
   WHERE ( dbo.productores.prod_codigo = dbo.sagdeclaproddeta.prod_codigo ) and  
         ( dbo.sagdeclaproddeta.espe_codigo = dbo.especies.espe_codigo ) and  
         ( ( dbo.sagdeclaproddeta.clie_codigo = :Cliente  ) AND  
         ( dbo.sagdeclaproddeta.plde_codigo = :Planta  ) AND  
         ( dbo.sagdeclaproddeta.espe_codigo = :especie  ) AND  
         ( dbo.sagdeclaproddeta.prod_codigo = :productor ) AND  
         ( dbo.sagdeclaproddeta.decl_fecdec = :fecha ) AND  
         ( dbo.sagdeclaproddeta.decl_predio = :predio ) )    
" update="dbo.sagdeclaproddeta" updatewhere=0 updatekeyinplace=yes arguments=(("cliente", number),("planta", number),("productor", number),("especie", number),("fecha", date),("predio", number)) )
