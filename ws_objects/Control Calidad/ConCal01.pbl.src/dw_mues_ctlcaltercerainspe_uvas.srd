 retrieve="    Select  lo.ccte_numero,lo.clie_codigo,lo.plde_codigo,lo.cclo_numero,lo.cctd_secuen,lo.cctd_embal1,
            lo.cctd_calemb,lo.cctd_calid1,lo.cctd_calcal,lo.cctd_condi1,lo.cctd_calcon,lo.cctd_califi,
            lo.cctd_respon,lo.cctd_cauobj,ho.ccho_tipins,ho.espe_codigo,lo.cctd_reclas,
            Max(ho.ccho_fecins),
            Max(ho.ccho_horins),
            ho.ccda_secuen,ho.ccho_folpla,lo.cctd_folpla,lo.cctd_tipins,lo.cctd_devpro,lo.cctd_otrare,
            lo.cctd_reclae,lo.cctd_resolu,lo.cctd_embal2,lo.cctd_calid2,lo.cctd_condi2,lo.cctd_repesa,
            lo.cctd_cammer,lo.cctd_resfin,lo.paen_numero, pr.prod_codigo
        From dbo.ctlcalotesobjetadosdet lo
        LEFT JOIN dbo.ctlcalhistobjetados ho ON 
        lo.plde_codigo = ho.plde_codigo AND 
        lo.cclo_numero = ho.cclo_numero AND   
        lo.clie_codigo = ho.clie_codigo AND
        lo.cctd_folpla = ho.ccho_folpla AND 
        lo.cctd_tipins = ho.ccho_tipins
        FULL JOIN dbo.ctlcallotes pr ON
        lo.cclo_numero = pr.cclo_numero
        WHERE :Planilla in(-1, lo.ccte_numero) 
        AND lo.plde_codigo =  :Planta
    Group by lo.ccte_numero,lo.clie_codigo,lo.plde_codigo,lo.cclo_numero,lo.cctd_secuen,lo.cctd_embal1,
             lo.cctd_calemb,lo.cctd_calid1,lo.cctd_calcal,lo.cctd_condi1,lo.cctd_calcon,lo.cctd_califi,
             lo.cctd_respon,lo.cctd_cauobj,ho.ccho_tipins,ho.espe_codigo,lo.cctd_reclas,ho.ccda_secuen,
             ho.ccho_folpla,lo.cctd_folpla,lo.cctd_tipins,lo.cctd_devpro,lo.cctd_otrare,lo.cctd_reclae,
             lo.cctd_resolu,lo.cctd_embal2,lo.cctd_calid2,lo.cctd_condi2,lo.cctd_repesa,lo.cctd_cammer,
    		    lo.cctd_resfin,lo.paen_numero, pr.prod_codigo" update="dbo.ctlcalotesobjetadosdet" updatewhere=0 updatekeyinplace=yes retrieve.asneeded=yes arguments=(("Planilla", number),("Planta", number)) )
