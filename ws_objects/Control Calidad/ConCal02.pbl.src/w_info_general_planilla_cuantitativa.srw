	Case -1, -9
		uo_SelProductor.Filtra(-1)
		uo_SelAgronomo.Filtra(-1)
		uo_SelInspectores.Filtra(-1)
		uo_SelPacking.Filtra(-1)
		
	Case Else
		uo_SelProductor.Filtra(This.Codigo)
		uo_SelAgronomo.Filtra(This.Codigo)
		uo_SelInspectores.Filtra(This.Codigo)
		uo_SelPacking.Filtra(This.Codigo)
End Choose
end event

