 retrieve="SELECT ce.plde_codigo, ce.cama_codigo, ce.esca_fecpro, ce.esca_secuen,
       ce.esca_estado, ce.emba_tipofr, cama_capaci=cb.cama_capmax, ce.cama_tipoca,   
       ce.esca_nropro, ce.esca_fecest, ce.usua_codigo, ce.esca_tipcam,
       cb.cama_nombre,   
       Convert(decimal(5), 0) as Actual,   
       IsNull(ce.esca_descar, 0) as esca_descar,   
       ce.esca_fecdes  
    FROM dbo.estibacamaraenca ce, dbo.camarasbode cb
   WHERE ce.plde_codigo = cb.plde_codigo
     and ce.cama_codigo = cb.cama_codigo
     And :Planta in (-1, ce.plde_codigo)
	AND :Camara in (-1, ce.cama_codigo)
	AND Datediff(dd, ce.esca_fecpro, :Fecha) = 0 
	AND :Secuencia in (-1, ce.esca_secuen)
" update="dbo.estibacamaraenca" updatewhere=0 updatekeyinplace=no arguments=(("Planta", number),("Fecha", datetime),("Camara", number),("Secuencia", number)) )
